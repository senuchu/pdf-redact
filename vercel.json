{
  "version": 2,
  "builds": [
    {
      "src": "api/redact.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/redact.py"
    },
    {
      "src": "/redact",
      "dest": "/api/redact.py"
    },
    {
      "src": "/download/(.*)",
      "dest": "/api/redact.py"
    }
  ]
}
